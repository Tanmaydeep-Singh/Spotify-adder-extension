{
  "short_name": "Spotify-adder",
  "name": "Spotify Adder",
  "icons": {
    "16": "favicon.ico",
    "48": "logo192.png",
   "128": "logo512.png"
  },
  "permissions": [
    "activeTab",
    "background",
    "storage",
    "tabs"
 ],
 "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["contentScript.js"]
  }
],
  "manifest_version": 3,
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html",
    "default_title": "Hello"
  }
}